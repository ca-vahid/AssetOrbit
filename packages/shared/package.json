{
  "name": "@ats/shared-transformations",
  "version": "1.0.0",
  "description": "Shared transformation library for asset import processing",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate-golden-masters": "jest --testNamePattern=\"Generate Golden Master Standards\" --verbose",
    "test-real-data": "jest --testNamePattern=\"Real Data Golden Master Tests\" --verbose",
    "generate-real-golden-masters": "jest --testNamePattern=\"Real Data Golden Master Generation\" --verbose",
    "validate-real-golden-masters": "jest --testNamePattern=\"Real Data Golden Master Validation\" --verbose",
    "generate-ninjaone-golden-masters": "jest --testNamePattern=\"NinjaOne Golden Master Generation\" --verbose",
    "validate-ninjaone-golden-masters": "jest --testNamePattern=\"NinjaOne Golden Master Validation\" --verbose"
  },
  "keywords": [
    "asset",
    "import",
    "transformation",
    "csv",
    "excel"
  ],
  "author": "AssetOrbit",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
